<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="classpath*:credentialbeans.xml" />

	<bean id="applicationContextProvider"
		class="eu.gloria.gs.services.api.security.ApplicationContextProvider"></bean>

	<bean id="hostName" class="java.lang.String">
		<constructor-arg value="host" />
	</bean>
	
	<bean id="hostPort" class="java.lang.String">
		<constructor-arg value="8443" />
	</bean>

</beans>
